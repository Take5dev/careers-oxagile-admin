{
  "collectionName": "components_settings_seos",
  "info": {
    "displayName": "SEO"
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "required": true
    },
    "metaDescription": {
      "type": "text",
      "required": true,
      "maxLength": 160
    },
    "canonicalUrl": {
      "type": "string"
    },
    "ogLocale": {
      "type": "string",
      "default": "en_US"
    },
    "ogType": {
      "type": "string",
      "default": "article"
    },
    "ogTitle": {
      "type": "string"
    },
    "ogDescription": {
      "type": "text"
    },
    "ogSiteName": {
      "type": "string"
    },
    "ogImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "author": {
      "type": "string",
      "default": "oxagile"
    },
    "twitterTitle": {
      "type": "string"
    },
    "twitterDescription": {
      "type": "text"
    },
    "twitterImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
