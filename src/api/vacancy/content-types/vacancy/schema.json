{
  "kind": "collectionType",
  "collectionName": "vacancies",
  "info": {
    "singularName": "vacancy",
    "pluralName": "vacancies",
    "displayName": "Vacancy",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "titleAdjective": {
      "type": "string"
    },
    "willdoText": {
      "type": "richtext"
    },
    "willdoTitle": {
      "type": "string"
    },
    "willdoShow": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "projectShow": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "weofferTitle": {
      "type": "string"
    },
    "weofferContent": {
      "type": "richtext"
    },
    "weofferShowButtton": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "weofferButtonLinkType": {
      "type": "enumeration",
      "enum": [
        "internal",
        "external"
      ],
      "default": "external",
      "required": false
    },
    "weofferButtonLink": {
      "type": "string"
    },
    "weofferButtonLabel": {
      "type": "string"
    },
    "weofferIcon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "isHot": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "image2": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "departments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::department.department",
      "inversedBy": "vacancies"
    },
    "projectTitle": {
      "type": "string"
    },
    "levels": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::level.level",
      "inversedBy": "vacancies"
    },
    "technologies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::technology.technology",
      "inversedBy": "vacancies"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "vacancies"
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "inversedBy": "vacancies"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "settings.seo",
      "required": true
    },
    "related_vacancies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vacancy.vacancy"
    },
    "Skills": {
      "type": "component",
      "repeatable": true,
      "component": "vacancies.skills"
    },
    "countries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::country.country",
      "mappedBy": "vacancies"
    }
  }
